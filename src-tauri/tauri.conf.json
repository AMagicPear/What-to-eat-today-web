{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "今天吃什么",
  "version": "3.0.0",
  "identifier": "com.amagicpear.dietmanager",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:8100",
    "beforeDevCommand": "ionic serve",
    "beforeBuildCommand": "ionic build"
  },
  "app": {
    "windows": [
      {
        "title": "今天吃什么",
        "width": 600,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
